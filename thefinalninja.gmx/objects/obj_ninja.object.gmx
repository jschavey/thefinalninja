            <string>//  Begin Input Handling

if ( Animation != Melee) {     // accept input only if not attacking
    switch (keyboard_key) {     // currently pressed key
        case vk_left:
            Facing = Left;
            Animation = Moving;
            break;
        case vk_right:
            Facing = Right;
            Animation = Moving;
            break;
        case ord('Z'):
            Animation = Melee;
            break;  
    }
    
    //  On Press events
    if ( keyboard_check_pressed( vk_left ) ) {
        Update = true;
    }
    if ( keyboard_check_pressed( vk_right ) ) {
        Update = true;
    }
    if ( keyboard_check_pressed( ord('Z') ) ) {
        Update = true;
    }
    if ( keyboard_check_pressed( vk_up ) &amp;&amp; y &gt; 544 )  {
        Animation = Jumping;
        Update = true;
        gravity = 1;        // play with these number to balance jump
        vspeed = -12;       
    }
    
    //  On Release events
    if ( keyboard_check_released( vk_left ) ) {
        Animation = Standing;
        Update = true;
    }
    if ( keyboard_check_released( vk_right ) ) {
        Animation = Standing;
        Update = true;
    }
}

if (Animation == Moving) {      // update position if moving
    if (Facing == Right) {
        x += 10;
    } else if (Facing == Left) {
        x -= 10;
    }
} 
</string>
